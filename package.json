{
  "name": "store",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clear": "npx jest --clearCache",
    "test:unit": "npx jest unit --passWithNoTests --watchAll --silent=false --config=jest.unit.config.js",
    "test:unit:cov": "npx jest unit --coverage --config=jest.unit.config.js",
    "test:int": "dotenv -e .env.test -- npx jest int --runInBand --passWithNoTests --watchAll --silent=true --config=jest.int.config.js",
    "test:int:cov": "dotenv -e .env.test -- npx jest int --runInBand --coverage --config=jest.int.config.js",
    "api:dev": "dotenv -e .env.dev -- npx ts-node-dev --files -r tsconfig-paths/register src/api/index.ts",
    "api:prod": "node -r dotenv/config -r ./tsconfig-paths-bootstrap.js ./build/src/api/index.js",
    "event_worker:dev": "dotenv -e .env.dev -- npx ts-node-dev -r tsconfig-paths/register src/event_worker/index.ts",
    "event_worker:prod": "node -r dotenv/config -r ./tsconfig-paths-bootstrap.js ./build/src/event_worker/index.js",
    "build": "tsc --project ./tsconfig.build.json",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate dev --skip-generate",
    "migrate:dev": "dotenv -e .env.dev -- npx prisma migrate dev --skip-generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hdev14/store.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hdev14/store/issues"
  },
  "homepage": "https://github.com/hdev14/store#readme",
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.1",
    "@types/node": "^18.8.2",
    "@types/nodemailer": "^6.4.6",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^29.1.2",
    "jest-mock-extended": "^3.0.1",
    "prisma": "^4.10.1",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@prisma/client": "^4.10.1",
    "axios": "^1.3.4",
    "bullmq": "^3.9.0",
    "express": "^4.18.1",
    "mongoose": "^6.6.4",
    "nodemailer": "^6.8.0",
    "tsconfig-paths": "^4.1.0"
  }
}
